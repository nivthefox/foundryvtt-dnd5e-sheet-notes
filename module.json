{
  "id": "dnd5e-sheet-notes",
  "title": "Sheet Notes DnD5e",
  "description": "Adds a Notes tab to D&D 5th Edition character sheets with integrated note-taking and tracker functionality",
  "version": "1.0.0",
  "compatibility": {
    "minimum": "12",
    "verified": "12"
  },
  "authors": [
    {
      "name": "nivthefox",
      "discord": "nivthefox"
    }
  ],
  "relationships": {
    "systems": [
      {
        "id": "dnd5e",
        "type": "system",
        "compatibility": {
          "minimum": "4.1.2"
        }
      }
    ],
    "requires": [
      {
        "id": "lib-wrapper",
        "type": "module",
        "compatibility": {
          "minimum": "1.12.0"
        }
      }
    ]
  },
  "esmodules": [
    "dist/main.js"
  ],
  "styles": [
    "dist/styles/notes.css"
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "dist/lang/en.json"
    }
  ],
  "manifest": "https://github.com/nivthefox/foundryvtt-dnd5e-sheet-notes/releases/latest/download/module.json",
